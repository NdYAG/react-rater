    "license": "BSD",
    "bugs": {
        "url": "https://github.com/NdYAG/react-rater/issues"
    },
    "homepage": "https://github.com/NdYAG/react-rater",
    "dependencies": {
        "react": "^0.12.1"
    },
    "devDependencies": {
        "react-tools": "^0.12.1"
    }
}
